<h1 class="right-align bottom">Edit Trail</h1>
<%= form_for @trail do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :mileage %>
  <%= f.text_field :mileage %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.submit class: "waves-effect light-green darken-4 btn" %>
<% end %>